{
  "name": "Talmud Sidebar Extension (Powered by Sefaria)",
  "version": "1.6.21",
  "manifest_version": 3,
  "description": "Bring the comprehensive Sefaria library to any webpage that is learning a specific Daf of Gemara.",
  "icons": {
    "16": "Icon_64.png",
    "48": "Icon_64.png",
    "128": "Icon_128.png"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": [
        "*://daf-yomi.com/*",
        "*://*.daf-yomi.com/*",
        "*://dafhachaim.org/*",
        "*://*.dafhachaim.org/*",
        "*://e-daf.com/*",
        "*://*.e-daf.com/*",
        "*://alldaf.org/*",
        "*://*.alldaf.org/*",
        "*://outorah.org/*",
        "*://*.outorah.org/*",
        "*://yutorah.org/*",
        "*://*.yutorah.org/*",
        "*://*.realcleardaf.com/*",
        "*://realcleardaf.com/*",
        "*://*.hadran.org.il/*",
        "*://hadran.org.il/*",
        "*://*.steinsaltz-center.org/*",
        "*://steinsaltz-center.org/*"
      ],
      "js": [
        "jquery-3.6.1.min.js",
        "recognizers.js",
        "contentScript.js"
      ],
      "css": [
        "SefariaSidebar.css"
      ],
      "run_at": "document_idle"
    }
  ]
}
