{
  "name": "Sefaria Sidebar Extension",
  "version": "1.6.7",
  "manifest_version": 2,
  "description": "Sefaria Sidebar Extension",
  "icons": {
    "16": "Sefaria_64x64.png",
    "48": "Sefaria_64x64.png",
    "128": "Sefaria_128x128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "Sefaria_64x64.png",
      "48": "Sefaria_64x64.png",
      "128": "Sefaria_128x128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://daf-yomi.com/*",
        "*://*.daf-yomi.com/*",
        "*://dafhachaim.org/*",
        "*://*.dafhachaim.org/*",
        "*://e-daf.com/*",
        "*://*.e-daf.com/*",
        "*://alldaf.org/*",
        "*://*.alldaf.org/*",
        "*://outorah.org/*",
        "*://*.outorah.org/*",
        "*://yutorah.org/*",
        "*://*.yutorah.org/*",
        "*://*.realcleardaf.com/*",
        "*://realcleardaf.com/*",
        "*://*.hadran.org.il/*",
        "*://hadran.org.il/*"
      ],
      "js": [
        "jquery-1.8.2.min.js",
        "recognizers.js",
        "contentScript.js"
      ],
      "css": [
        "SefariaSidebar.css"
      ],
      "run_at": "document_end"
    }
  ]
}